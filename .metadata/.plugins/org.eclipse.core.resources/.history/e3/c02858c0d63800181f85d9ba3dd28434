package com.gammas.geoex.items;

import com.gammas.geoc.api.item.ICrushable;

import net.minecraft.item.Item;

public class ItemCrushable extends BaseItem implements ICrushable{

	private Item crushedResult;
	
	public ItemCrushable(String name, Item crushedResult) {
		super(name);
		
		this.crushedResult = crushedResult;
	}

	@Override
	public Item getCrushedResult() {
		return crushedResult;
	}

}
